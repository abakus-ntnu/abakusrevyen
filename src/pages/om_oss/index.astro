---
import PageLayout from "@/layouts/PageLayout.astro";
import { getCollection, getEntry, render  } from "astro:content";

const getListCollection = async (collectionName) => {
    const collection = await getCollection(collectionName);
    return collection.map(
        (entry) => `<li><a href="/om_oss/${entry.id}">${entry.data.title}</a></li>`
    ).join('');
};
const oss = await getCollection('oss');
console.log("OSS Collection Entries:", oss);
const collectionOss = await getListCollection('oss')
