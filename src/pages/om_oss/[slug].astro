---
import PageLayout from "@/layouts/PageLayout.astro";
import { getEntry, render, getCollection } from "astro:content";

export async function getStaticPaths() {
    const entries = await getCollection('oss');
    
    const list = entries
    .map(e => ({
        params: { slug: e.id.toString() } // Convert `id` to a string if necessary
    }));

    return list;
}

const { slug } = Astro.params;
const oss = await getEntry('oss', slug)
const { Content, headings } = await render(oss);
---

<PageLayout title="Om oss">
    <Content />
</PageLayout>